{
  "name": "react-template-ice2",
  "version": "0.0.1",
  "description": "梨花模板",
  "scripts": {
    "tsc": "tsc --project config/tsconfig.json",
    "tsc:watch": "tsc --project config/tsconfig.json -w",
    "start:watch": "concurrently \"npm run tsc:w\" \"icejs start --config .build.config/config.js\"",
    "start:w": " npm-run-all --parallel tsc:watch start",
    "start": "npm run tsc && icejs start --config .build.config/config.js --mode dev",
    "start1": "icejs start",
    "build": "npm run tsc && icejs build --config .build.config/config.js",
    "build:analyzer": "npm run tsc && icejs build --config .build.config/config.js --analyzer",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "upload": "node ./ssh/index.js react-template-ice2",
    "prettier": "prettier --config .prettierrc.js --write \"./src/**/*.{js,sjs,ts,tsx,wxs,css,wxss,acss,wxml,axml,less,scss,json}\""
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.14",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-table": "^2.56.9",
    "@rematch/core": "^2.2.0",
    "@rematch/immer": "^2.1.3",
    "@rematch/loading": "^2.1.2",
    "@rematch/persist": "^2.1.2",
    "antd": "^4.16.13",
    "axios": "^0.24.0",
    "build-plugin-ignore-style": "^0.1.1",
    "build-plugin-keep-alive": "^1.8.1",
    "intersection-observer": "^0.12.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-css-modules": "^4.7.11",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/postcss-import": "^12.0.1",
    "@types/postcss-preset-env": "^6.7.3",
    "@types/postcss-url": "^8.0.2",
    "@types/react": "^17.0.2",
    "@types/react-css-modules": "^4.6.4",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.20",
    "concurrently": "^6.2.1",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^7.30.0",
    "ice.js": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "8.0.0",
    "postcss-viewport-units": "^0.1.6",
    "stylelint": "^13.7.2",
    "typescript": "^4.4.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/react-materials/tree/master/scaffolds/simple"
  },
  "private": true,
  "originTemplate": "@alifd/scaffold-simple"
}
